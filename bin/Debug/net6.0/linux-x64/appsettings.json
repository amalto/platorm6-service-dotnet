{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Console": {
    "LogLevel": { // Console provider
      "Default": "Debug", // Overrides preceding LogLevel:Default setting.
      "Microsoft": "Trace" // Writes at Trace Level inside the Microsoft assembly
    }
  },
  "AllowedHosts": "*"
}

